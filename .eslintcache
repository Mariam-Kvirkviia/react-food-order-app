VE\", id: id });\r\n  };\r\n  let [showModal, setModal] = useState(false);\r\n\r\n  let showingModal = () => {\r\n    setModal(true);\r\n  };\r\n  let hidingModal = () => {\r\n    setModal(false);\r\n  };\r\n  return (\r\n    <Context.Provider\r\n      value={{\r\n        items: cartState.items,\r\n        totalAmount: cartState.totalAmount,\r\n        addItem: addItemToCart,\r\n        removeItem: removeItemFromCart,\r\n      }}\r\n    >\r\n      <Header onSetModal={showingModal} />\r\n      <Meals />\r\n      {showModal && <Modal onSetModal={hidingModal} />}\r\n    </Context.Provider>\r\n  );\r\n};\r\nexport default App;\r\n","C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\header\\Header.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\header\\button\\HeaderCartButton.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\header\\button\\CartIcon.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\meal\\MealsSummary.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\meal\\MealItem.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\meal\\AvailableMeals.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\meal\\Meals.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\UI\\Card.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\meal\\MealItemForm.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\UI\\input\\input.js",[],["80","81"],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\UI\\input\\Input.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\cart\\Cart.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\cart\\CartItem.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\components\\meal\\Modal.js",[],"C:\\Users\\marik\\Desktop\\react-food-order-app\\src\\Context.js",[],"C:\\U